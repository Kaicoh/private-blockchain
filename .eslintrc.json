{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": ["error", 4],
        "func-names": 0,
        "no-console": 0,
        "prefer-arrow-callback": 0,
        "spaced-comment": 0,
        "no-plusplus": 0
    }
}